page{
  background-color: #222946;
}
.title{
   text-align: center;
   height: 40px;
   color: #fff;
   line-height: 40px;
   font-size: 18px;
   background-color: rgb(37, 105, 186);
   font-weight: 600;
}
.open{
  font-size: 14px;
  color: #999;
}
.map{
   width: 10px;
   height:10px;
}
.openread{
  float: right;
  margin-right: 10px;
  margin-top: 10px;
}

.content{
  padding: 10px;
  line-height: 25px;
  text-indent: 0px;
  font-size: 14px;
  color: #666;
}
.panda{
  display: block;
  width: 100%;
}
.pandatitle{
  border-top-left-radius: 10px;
  border-top-right-radius: 10px;
  font-size: 20px;
  text-align: center;
  
  height: 45px;
  line-height: 45px;
  color: white;
  font-weight: 600;
  background-color: rgb(84, 190, 94);
}
.pandaContainer{
   padding-bottom:10px; 
   border-radius: 10px;
   margin-left: 15px;
   margin-right: 15px;
   margin-top: 5px;
   background-color: white;
}
.heart{
  margin-top: 3px;
  padding-left: 5px;
  width: 24px;
  height: 22px;
}
.heartContainer{
   display: flex;
   margin-top: 10px;
}
.heartArea{
  display: flex;
  flex-direction: row;
  margin-left: 15px;
  background-color: rgb(84, 190, 94);
  width: 120px;
  font-weight: 400
}
.heartCount{
  font-weight: 600;
  font-size: 20px;
  padding-left: 5px;
  color: white;
}
.support{
  margin-left: 35%;
  width: 80px;
  height: 30px;
}

.prime{
  background-color: white;
  margin-left: 15px;
  margin-right: 15px;
  margin-top: 5px;
  border-radius: 10px;
}
.primetitle{
  text-align: center;
  background-color: rgb(0, 191, 243);
  border-top-left-radius: 10px;
  border-top-right-radius: 10px;
  font-size: 20px;
  height: 45px;
  line-height: 45px;
  color: white;
  font-weight: 600;
}
.primeSupp{
   width: 80px;
   height: 30px;
   margin-bottom: 10px;
}
/* .primeunSupp{
   margin-bottom: 10px;
   width: 76px;
   height: 26px;
} */
.people{
 display: flex;
}
.primeone{
  border-bottom-left-radius: 10px;
  border-bottom-right-radius: 10px;
  width: 50%;
  text-align: center;
  background-color: rgb(214, 226, 228);
}
.primeone:nth-child(2n+1){
    background-color: rgb(159, 237, 251)
}
.nick{
  padding-top: 20px;
  padding-bottom: 10px;
  font-size: 14px;
  font-family: "黑体";
  font-weight: bold;
}
.introduce{
  padding-bottom: 10px;
  font-size: 12px;
  color: rgb(123, 147, 154)
}

.ticket{
  margin-left: 40px;
  width: 55%;
  margin-bottom: 10px;
  background-color: rgb(0, 145, 255);
  color: white;
}
.vs{
  height: 90px;
  width: 60px;
}


.hello{
   text-align: center;
}
.vsarea{
  position: absolute;
  left: 42%;
}
.party{
  text-align: center;
  background-color: rgb(37, 105, 168);
  border-top-left-radius: 10px;
  border-top-right-radius: 10px;
  font-size: 20px;
  height: 45px;
  line-height: 45px;
  color: white;
  font-weight: 600;
}
.partyintro{
   padding-top: 20px;
   padding-bottom: 10px;
   font-weight: 500;
   font-size: 14px;
   color: rgb(0, 145, 255);
   text-decoration: underline;
}
.buttonarea{
   margin-top: 15px;
   margin-bottom: 15px;
}
.button{
  background-color: rgb(247, 181, 0);
  width: 160px;
  color: white;
}
.btnSupp{
  line-height: 30px;
  width: 80px;
  height: 30px;
  color: #fff;
  border-radius: 12px;
  background-color: rgb(109, 114, 120);
  margin-bottom: 10px;
}
.btnSupp:checked{
  line-height: 30px;
  width: 80px;
  height: 30px;
  color: #fff;
  border-radius: 12px;
  background-color: rgb(19, 30, 133);
  margin-bottom: 10px;
}
.he{
  width: 100px;
  height: 100px;
  border-radius: 50%;
}